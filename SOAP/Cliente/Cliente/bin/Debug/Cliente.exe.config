<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="WSAndroidPortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/WSAndroid/WSAndroid"
                binding="basicHttpBinding" bindingConfiguration="WSAndroidPortBinding"
                contract="ws.WSAndroid" name="WSAndroidPort" />
        </client>
    </system.serviceModel>
</configuration>